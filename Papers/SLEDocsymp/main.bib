 doi = {http://doi.acm.org/10.1145/1368088.1368131},
 acmid = {1368131},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {feature refactoring, ide, software product lines, virtual separation of concerns},
 annote = "In this paper authors look at the problems encountered in creating software product lines from legacy applications.
 One of the major challenges is the fine-grained extensions. They present their tool called Colored IDE to support granularity in SPLs.
 SPL implementation approaches are divided into two groups, compositional approach and annotative approach which both has their limitations. 
 Authors talk about the problem of coarse granularity and the difficulty of fine-grained extention support. They stress the limitations of programming languages
 and present their approach on decomposing legacy applications into features that may have fine granularity. Instead of using annotations authors use colors to associate features with 
 code. They use a preprocessor like approach to decompose the source code in an Abstract Syntac Tree, and allow the user to make fine grained changes to the code. "
} 



@ARTICLE{Apel:aspectualfeat, 
author={Apel, S. and Leich, T. and Saake, G.}, 
journal={Software Engineering, IEEE Transactions on}, 
title={Aspectual Feature Modules}, 
year={2008}, 
month={march-april }, 
volume={34}, 
number={2}, 
pages={162 -180}, 
keywords={C++;Java;aspect-oriented programming;aspectual feature module;crosscutting concern;feature-oriented programming;incremental software development;large-scale compositional programming;large-scale software building block;software product line;systematic evaluation;object-oriented programming;software reusability;}, 
doi={10.1109/TSE.2007.70770}, 
ISSN={0098-5589},
annote = "FOP and AOP are complementary and can benefit from each other. The paper first compares FOP and AOP using three criteria. Homogeneous and Heterogeneous crosscuts, Static and Dynamic crosscuts
and Feature Cohesion. The main idea drawn from this evaluation is FOP and AOP complements each others' weaknesses. Aspectual feature modules are a concrete approach to implementing the symbiosis of FOP and AOP. 
The authors employ an architectural approach because they forsee AFMs can evolve with further research in FOP and AOP. An in-language approach would be too expensive to modify. 
In AFM aspects are encapsulated in feature modules. In conclusion; dominant role of features is the introduction of new structural elements, aspects lead to reduction in the amount of code and cleaner design and implementation.  "


}


